
@{
    ViewBag.Title = "Weekly";
}



    <div id="accordion" role="tablist">
        @foreach (var Prayer in Model)
        {
        <div class="card">
            <div class="card-header" role="tab" id="@Prayer.FeedbackEntryID">
            <a data-toggle="collapse" href="#@Prayer.FeedbackEntryID" aria-expanded="true" aria-controls="collapseOne">
                @Prayer.DisplayName
            </a>
            </div>

        <div id="@Prayer.FeedbackEntryID" class="collapse" role="tabpanel" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
                <div>Email: <a href="mailto:@Prayer.EmailAddress">@Prayer.EmailAddress</a></div>
                <div>Mobile Phone: @Prayer.MobilePhone</div>
                <div>Home Phone: @Prayer.HomePhone</div><br />
                <div>Membership: @Prayer.MembershipStatus</div>
                <div>Parish: @Prayer.ParishName</div>
                <div>Shepherd: @Prayer.Shepherd</div><br />
                <div class="text-muted">@Prayer.Description</div>
            </div>
        </div>
        </div>
        }
    </div>



